- @details.each do |cur|
  %tr.report_tasks_td_ditails{:class => "#{params[:id]}_#{params[:user_id]}_#{params[:project_id]}_details"}
    - cur.descriptions.each do |description|
      %td
      %td
        = description.description_name
      %td
        = cur.start_time.strftime("%Y/%m/%d %H:%M")
        &mdash;
        = cur.end_time.strftime("%Y/%m/%d %H:%M")
